package numericData;

public class RectangleComputation {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		 
		double length, width, area, perimeter;
		length = 10.5;
		width = 11.5;
		
		area = length*width;
		perimeter = length+length+width+width;
		
		System.out.println("The area is: " + area);
		System.out.println("The perimeter is: " + perimeter);

	}

}

package numericData;

public class CylinderComputation {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		double radius, height, surfaceArea, baseArea, volume;
		radius = 5.5;
		height = 9;
		final double PI = 3.14159265;
		
		surfaceArea = 2*PI*radius*(height+radius);
		baseArea = PI*(radius*radius);
		volume = PI*(radius*radius)*height;
		
		System.out.printf("The surface area is: %.2f" , surfaceArea);
		System.out.printf("\nThe base area is: %.2f " , baseArea);
		System.out.printf("\nThe volume is: %.2f" , volume);

	}

}
